{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "path": "[[workspace]]",
      "title": "[[workspace]]"
    },
    {
      "path": "[[workspace]]"
    },
    {
      "title": "posts",
      "path": "[[workspace]]/src/content/posts"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "Touch a new post",
      "pageBundle": false,
      "previewPath": "'blog'",
      "filePrefix": true,
      "clearEmpty": true,
      "fields": [
        {
          "title": "title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "pubDate",
          "name": "pubDate",
          "type": "datetime"
        },
        {
          "title": "categories",
          "name": "categories",
          "type": "categories"
        },
        {
          "title": "description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        }
      ]
    }
  ],
  "frontMatter.framework.startCommand": "npm run build",
  "frontMatter.dashboard.openOnStart": true,
  "frontMatter.git.enabled": true,
  "frontMatter.website.host": "https://iamalex.blue"
}
